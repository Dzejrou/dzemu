$string hello "Hello, world!"

.byte __tmp $00 $00 $00 $00 $00 $00 $00
.byte __num $00

.macro print __var
  $mov __num __var
  JSR print_num

__clear_tmp:
LDX #$07
LDA #$00
__clear_loop:
STA __tmp, X
DEX
BNE __clear_loop
RTS

print_num:
LDA __num
TOS __tmp
PRT __tmp
JSR __clear_tmp
RTS

.byte var1 $EF
.byte var2 $09
.byte var3 $AD

start:

PRT hello

$print var1
$print var2
$print var3
