.include "print.a65"
.include "math.a65"

.byte loop_count $05
.byte hello $48 $45 $4C $4C $4F $00
.byte hallo $48 $41 $4C $4C $4F $00
.byte start_addr $20

.byte push_test $05
.byte pop_test $04

.byte my_print_count $02

.byte mov_var1 $A5
.byte mov_var2 $7D

$string hey_str "hey there"
$string __print_string "test string"

.macro test_arg _arg1 _arg2
