; Prints the string to stdout.
print:
LDX loop_count
print_loop:
PRT $F0
DEX
BNE print_loop
RTS

; Loads "HELLO" to the memory address 0xFO.
load_hello:
LDA #$48
STA *$F0
LDA #$45
STA *$F1
LDA #$4C
STA *$F2
LDA #$4C
STA *$F3
LDA #$4F
STA *$F4
RTS

; Loads "HALLO" to the memory address 0xF0.
load_hallo:
LDA #$48
STA *$F0
LDA #$41
STA *$F1
LDA #$4C
STA *$F2
LDA #$4C
STA *$F3
LDA #$4F
STA *$F4
; Change the loop count.
LDA #$02
STA loop_count ; Variables work across files!
RTS
